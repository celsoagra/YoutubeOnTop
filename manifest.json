{
  "name": "YoutubeOnTop",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Enjoy the web while watching youtube!",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
	"96": "icon.png",
	"128": "icon_128.png"
  },
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  }
}