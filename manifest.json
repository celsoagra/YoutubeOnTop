{
  "name": "YoutubeOnTop",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Enjoy the web while watching youtube!",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": { "96": "icon.png"},
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"]
  }
}